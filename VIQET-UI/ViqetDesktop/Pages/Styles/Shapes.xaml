<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="BlueStyle" TargetType="Ellipse">
        <Setter Property="Fill" Value="#FF6790C9"/>
        <!--<Setter Property="Fill" Value="#FF687C7F"/>-->
    </Style>

    <Style x:Key="PurpleStyle" TargetType="Ellipse">
        <Setter Property="Fill" Value="#FF6F67C9"/>
    </Style>

    <Style x:Key="OrangeStyle" TargetType="Ellipse">
        <Setter Property="Fill" Value="Orange"/>
    </Style>
</ResourceDictionary>